[
  {
    "kernelSource": {
      "name": "GXC-Kernel",
      "repo": "https://github.com/GXC2356/kernel_xiaomi_vayu.git",
      "branch": "ksu",
      "device": "vayu",
      "defconfig": "vayu_defconfig"
    },
    "withKernelSU": true,
    "toolchains": [
      {
        "repo": "https://gitlab.com/crdroidandroid/android_prebuilts_clang_host_linux-x86_clang-r498229b.git",
        "branch": "13.0",
        "name": "clang",
        "binPath": ["bin"]
      },
      {
        "repo": "https://github.com/LineageOS/android_prebuilts_gcc_linux-x86_aarch64_aarch64-linux-android-4.9",
        "branch": "lineage-19.1",
        "name": "gcc",
        "binPath": ["bin"]
      }
    ],
    "ccache": false,
    "params": {
      "ARCH": "arm64",
      "CC": "clang/bin/clang",
      "externalCommand": {
        "LLVM": "1",
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "arm-linux-androideabi-",
        "CROSS_COMPILE_COMPAT": "arm-linux-androideabi-"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "repo": "https://github.com/sanjeevstunner/AnyKernel3",
      "branch": "master"
    }
  }
]
